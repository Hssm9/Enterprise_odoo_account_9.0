<?xml version="1.0"?>
<odoo>

    <!--费用支出FORM-->
    <record model="ir.ui.view" id="dtdream_sale_fee_pay_form">
        <field name="name">dtdream.sale.fee.pay.form</field>
        <field name="model">fee.pay</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name" string="费用支出(万元)"/>
                            <field name="pay_project_id" invisible="1"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <!--费用支出TREE-->
    <record model="ir.ui.view" id="dtdream_sale_fee_pay_tree">
        <field name="name">dtdream.sale.fee.pay.tree</field>
        <field name="model">fee.pay</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" string="费用支出(万元)"/>
                <field name="create_date" string="创建时间"/>
            </tree>
        </field>
    </record>

</odoo>