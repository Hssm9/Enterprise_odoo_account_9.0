        <field name="view_mode">tree,form</field>
        <field name="context">{
                        'default_gaap_project_id': active_id
        }</field>
        <field name="domain">[('gaap_project_id','=',active_id)]</field>
    </record>

    <!--合同额豆腐块跳转动作-->
    <record id="action_contract_fee" model="ir.actions.act_window">
        <field name="name">合同额</field>
        <field name="res_model">contract.fee</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{
                        'default_contract_project_id': active_id
        }</field>
        <field name="domain">[('contract_project_id','=',active_id)]</field>
    </record>

    <!--费用支出豆腐块跳转动作-->
    <record id="action_fee_pay" model="ir.actions.act_window">
        <field name="name">费用支出</field>
        <field name="res_model">fee.pay</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{
                        'default_pay_project_id': active_id
        }</field>
        <field name="domain">[('pay_project_id','=',active_id)]</field>
    </record>
</odoo>