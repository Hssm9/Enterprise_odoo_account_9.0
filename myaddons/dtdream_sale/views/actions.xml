<?xml version="1.0"?>
<odoo>
    <!--现金收入豆腐块跳转动作-->
    <record id="action_cash_income" model="ir.actions.act_window">
        <field name="name">现金收入</field>
        <field name="res_model">cash.income</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{
                        'default_cash_project_id': active_id
        }</field>
        <field name="domain">[('cash_project_id','=',active_id)]</field>
    </record>

    <!--GAAP收入豆腐块跳转动作-->
    <record id="action_gaap_income" model="ir.actions.act_window">
