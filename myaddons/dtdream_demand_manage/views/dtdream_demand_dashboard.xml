<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data>
        <template id="assets_backend" name="dtdream_remand assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/dtdream_demand_manage/static/src/css/style.css"/>
                <script type="text/javascript" src="/dtdream_demand_manage/static/src/js/dtdream_demand_dashboard.js"/>
            </xpath>
        </template>

        <record id="view_dtdream_remand_dashboard_kanban" model="ir.ui.view">
            <field name="name">view.dtdream.remand.dashboard.kanban</field>
            <field name="model">dtdream.remand.dashboard</field>
            <field name="type">remand_dashboard</field>
            <field name="arch" type="xml">
                <remand_dashboard create="0">
                </remand_dashboard>
            </field>
        </record>


        <record model="ir.actions.act_window" id="act_dtdream_demand_dashboard">
            <field name="name">仪表盘</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">dtdream.remand.dashboard</field>
            <field name="view_type">form</field>
            <field name="view_mode">remand_dashboard</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

        <menuitem name="仪表盘" id="menu_dtdream_demand_dashboard" parent="menu_dtdream_demand_manage_root" sequence="0" action="act_dtdream_demand_dashboard"/>

    </data>
</openerp>