<?xml version="1.0" encoding="utf-8" ?>
<templates>

    <t t-name="dtdream_demand_manage.RemandDashboardView">
        <div class="yf">
            <div class="container">
                <h6>应用开发及优化和域群组管理</h6>
                <div class="row interval" >
                    <div class="col-md-3 col-sm-6 col-xs-12">
                        <div class="panel" style="border: 0px;" name="dtdream_demand_manage.act_dtdream_demand_manage">
                            <a href="#">
                                <div class="panel-footer panelbox-blue">
                                    <h4>我相关的应用开发及优化</h4>
                                    <div class="yf-t"><t t-esc="demand_number"/></div>
                                    <div class="yf-more">
                                        <span>显示明细</span>
                                        <img src="/dtdream_demand_manage/static/src/img/dodo_arrow.png" alt=""/>
                                    </div>
                                </div>
                            </a>
                        </div>
                    </div>
                    <div class="col-md-3 col-md-offset-1 col-sm-6 col-xs-12" >
                        <div class="panel" style="border: 0px;"  name="dtdream_ad_manager.act_ad_group">
                            <a href="#">
                                <div class="panel-footer panelbox-red">
                                    <h4>我相关的域群组</h4>
                                    <div class="yf-t"><t t-esc="group_number"/></div>
                                    <div class="yf-more panel" name="dtdream_ad_manager.act_ad_group_apply">
                                        <span>申请创建域群组</span>
                                        <img src="/dtdream_demand_manage/static/src/img/dodo_arrow.png" alt=""/>
                                    </div>
                                </div>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </t>

</templates>