<openerp>
  <data>
    <record id="view_dtdream_project_order_form" model="ir.ui.view">
        <field name="name">view.dtdream.project.order.form</field>
        <field name="model">dtdream.project.order</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="code"/>
                        <field name="order_type"/>
                        <field name="bom"/>
                        <field name="vender"/>
                    </group>
                    <group>
                        <field name="version"/>
                        <field name="product_type"/>
                        <field name="company"/>
                        <field name="num"/>
                    </group>
                    <group colspan="12">
                        <field name="product_des"/>
                    </group>
                </group>
            </form>
        </field>
    </record>

  </data>
</openerp>