<?xml version="1.0"?>
<odoo>

    <!--回款豆腐块跳转动作-->
    <record id="action_cash_income" model="ir.actions.act_window">
        <field name="name">回款</field>
        <field name="res_model">dtdream.sale.cash.income</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{
                        'default_cash_project_id': active_id
        }</field>
        <field name="domain">[('cash_project_id','=',active_id)]</field>
    </record>


    <!--定义回款action-->
    <record id="dtdream_sale_cash_income_action" model="ir.actions.act_window">
        <field name="name">回款</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">dtdream.sale.cash.income</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>