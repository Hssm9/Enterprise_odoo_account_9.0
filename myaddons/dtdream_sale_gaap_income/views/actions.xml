<?xml version="1.0"?>
<odoo>

    <!--GAAP收入豆腐块跳转动作-->
    <record id="action_gaap_income" model="ir.actions.act_window">
        <field name="name">GAAP收入</field>
        <field name="res_model">dtdream.sale.gaap.income</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{
                        'default_gaap_project_id': active_id
        }</field>
        <field name="domain">[('gaap_project_id','=',active_id)]</field>
    </record>


    <!--定义GAAP收入action-->
    <record id="dtdream_sale_gaap_income_action" model="ir.actions.act_window">
        <field name="name">GAAP收入</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">dtdream.sale.gaap.income</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>