                    <field name="name">view.dtdream.contract.wizard</field>
                    <field name="model">dtdream.contract.wizard</field>
                    <field name="arch" type="xml">
                        <form>
                            <group>
                                <group>
                                    <field name="reason" required="1"></field>
                                    <field name="temp" invisible="1"></field>
                                </group>
                            </group>
                            <footer>
                                <button name="btn_confirm" type="object" string="确定" class="oe_highlight zx_button" style="border-radius:5px" id="zx_button"></button>
                            </footer>
                        </form>

                    </field>
                </record>
