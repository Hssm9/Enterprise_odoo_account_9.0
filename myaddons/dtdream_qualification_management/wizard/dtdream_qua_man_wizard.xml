<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_dtdream_qua_man_wizard_submit_from" model="ir.ui.view">
            <field name="name">view.dtdream.qua.man.wizard.form</field>
            <field name="model">dtdream.qua.man.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="dead_line" placeholder="请填写截止时间"/>
                        </group>
                    </group>
                    <footer>
                        <button name="btn_confirm" type="object"
                                string="确认" class="oe_highlight"/>
                        <button special="cancel" string="取消"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="view_dtdream_qua_man_wizard_finish_from" model="ir.ui.view">
            <field name="name">view.dtdream.qua.man.wizard.finish.form</field>
            <field name="model">dtdream.qua.man.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <p>确定完成？</p>
                    </group>
                    <footer>
                        <button name="btn_finish" type="object"
                                string="确认" class="oe_highlight"/>
                        <button special="cancel" string="取消"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="view_dtdream_qua_man_wizard_email_from" model="ir.ui.view">
            <field name="name">view.dtdream.qua.man.wizard.email.form</field>
            <field name="model">dtdream.qua.man.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <p>确定邮催未完成资料填写人员？</p>
                    </group>
                    <footer>
                        <button name="btn_email" type="object"
                                string="确认" class="oe_highlight"/>
                        <button special="cancel" string="取消"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</openerp>