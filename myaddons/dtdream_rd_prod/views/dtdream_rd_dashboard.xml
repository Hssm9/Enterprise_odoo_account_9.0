<?xml version="1.0" encoding="UTF-8" ?>
<openerp>
    <data>
        <template id="assets_backend" name="dtdream_rd assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/dtdream_rd_prod/static/src/css/expense.css"/>
                <script type="text/javascript" src="/dtdream_rd_prod/static/src/js/rd_dashboard.js"></script>
            </xpath>
        </template>

        <record id="dtdream_rd_dashboard_view_kanban" model="ir.ui.view">
            <field name="name">dtdream.rd.dashboard</field>
            <field name="model">dtdream.rd.dashboard</field>
            <field name="arch" type="xml">
                <rd_dashboard create="0">
                </rd_dashboard>
            </field>
        </record>


        <record model="ir.actions.act_window" id="act_open_dtdream_rd_dashboard_view">
            <field name="name">仪表盘</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">dtdream.rd.dashboard</field>
            <field name="view_type">form</field>
            <field name="view_mode">rd_dashboard,form</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
        </record>

    </data>
</openerp>